{%- assign object = section -%}
{%- liquid
   assign animation_row = 1
   assign drawer_position = 'right'

   if logo_alignment == 'center'
     assign drawer_position = 'left'
   endif

   assign filtered_block = object.blocks | where: 'type', 'logo'
   assign logo_block = filtered_block[0]

   assign drawer_logo_size = logo_block.settings.drawer_logo_width | append: 'x'
-%}
{% style %}
  .slide-nav__wrapper {
    --initial-height: calc((((var(--typeBaseSize)*.92)*var(--typeBaseLineHeight)) + 32px) * {{ main_menu.links.size }});
  }
{% endstyle %}
<div id="NavDrawer" class="drawer drawer--{{ drawer_position }}">
  <div class="drawer__contents">
    <div class="drawer__fixed-header">
      <div class="drawer__header appear-animation appear-delay-{{ animation_row }}">
        {% if logo_block.settings.logo %}
        <div class="drawer__header--logo--container ">
          <a
            href="{{ routes.root_url }}"
            itemprop="url"
            class="site-header__logo-link"
            style="justify-content: center;">
            <img
              class="medium-up--hide"
              src="{{ logo_block.settings.logo | img_url: drawer_logo_size }}"
              srcset="{{ logo_block.settings.logo | img_url: drawer_logo_size }} 1x, {{ logo_block.settings.logo | img_url: drawer_logo_size, scale: 2 }} 2x"
              alt="{{ logo_block.settings.logo.alt | default: shop.name }}">
          </a>
        </div>
        {% endif %}
        <div class="h2 drawer__title">
          <div class="slide-nav__search-field appear-animation appear-delay-1">
            <button class="slide-nav__search-trigger">
              <span class="search-icon--wrapper">
                <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-search" viewBox="0 0 64 64">
                  <path d="M47.16 28.58A18.58 18.58 0 1 1 28.58 10a18.58 18.58 0 0 1 18.58 18.58zM54 54L41.94 42"></path>
                </svg>
              </span>
              {{ 'general.search.placeholder' | t }}
            </button>
          </div>
          <span class="title__placeholder"></span>
        </div>
        <div class="drawer__close">
          <button type="button" class="drawer__close-button js-drawer-close">
            <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-close" viewBox="0 0 64 64">
              <path d="M19 17.61l27.12 27.13m0-27.12L19 44.74"></path>
            </svg>
            <span class="icon__fallback-text">{{ 'general.drawers.close_menu' | t }}</span>
          </button>
        </div>
      </div>
    </div>
    <div class="drawer--item__wrapper">
      <div class="slide-nav__wrapper" data-level="1">
        <ul class="slide-nav">
          {%- for link in main_menu.links -%}
            {%- liquid
               assign animation_row = animation_row | plus: 1
               assign link_index = forloop.index
               assign child_list_handle = link.title | handleize | append: link_index
               
               assign is_collection = false
               if link.url contains '/collections/'
                 assign lang_code_string = request.locale.iso_code | prepend: '/' | downcase
                 assign subcollection_handle = link.url | remove: '/collections/' | remove: lang_code_string
                 assign subcollection_drop = collections[subcollection_handle]
                 assign is_collection = true
               endif
            -%}
            <li class="slide-nav__item mobile-nav__item appear-animation appear-delay-{{ animation_row }}">
              {%- if link.levels > 0 -%}
                <button type="button" class="slide-nav__button js-toggle-submenu" data-target="tier-2-{{ child_list_handle }}" data-title="{{ link.title | escape }}" {% if link.active or link.child_active %}data-active="true"{% endif %}>
                  <span class="slide-nav__link  mobile-nav__link--top-level">
                    <span>{{ link.title }}</span>
                    <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right" viewBox="0 0 284.49 498.98">
                      <path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path>
                    </svg>
                    <span class="icon__fallback-text">{{ 'general.drawers.expand_submenu' | t }}</span>
                  </span>
                </button>
                <ul class="slide-nav__dropdown" data-parent="tier-2-{{ child_list_handle }}" data-level="2">
                  <li class="slide-nav__item">
                    <button type="button" class="slide-nav__button js-toggle-submenu">
                      <span class="slide-nav__link slide-nav__link--back">
                        <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-left" viewBox="0 0 284.49 498.98">
                          <path d="M249.49 0a35 35 0 0 1 24.75 59.75L84.49 249.49l189.75 189.74a35.002 35.002 0 1 1-49.5 49.5L10.25 274.24a35 35 0 0 1 0-49.5L224.74 10.25A34.89 34.89 0 0 1 249.49 0z"></path>
                        </svg>
                        <span>{{ 'general.drawers.mobile_back_arrow_text' | t }}</span>
                      </span>
                    </button>
                  </li>
                  {% comment %}
                    If a collection, automatically add 'view all' link
                  {% endcomment %}
                  {%- if is_collection -%}
                    <li class="slide-nav__item">
                      <a href="{{ link.url }}" class="slide-nav__link">
                        <span>{{ 'collections.general.all_of_collection' | t }}</span>
                        <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right" viewBox="0 0 284.49 498.98">
                          <path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path>
                        </svg>
                      </a>
                    </li>
                  {%- endif -%}
                  {%- for childlink in link.links -%}
                    {%- liquid
                      assign grand_child_list_handle = childlink.title | handleize
                      assign grand_child_link_id = grand_child_list_handle | append: link_index
                    -%}
                    <li class="slide-nav__item mobile-nav__item">
                      {%- if childlink.levels > 0 -%}
                        <button type="button" class="slide-nav__button js-toggle-submenu" data-target="tier-3-{{ grand_child_link_id }}" data-title="{{ childlink.title | escape }}" {% if link.active or link.child_active %}data-active="true"{% endif %}>
                          {%- assign handlize_link = childlink.title | handle -%}
                          <span class="slide-nav__link slide-nav__sublist-link">
                            <span class="{% if handlize_link == 'sale' %}link--danger{% endif %}">{{ childlink.title }}</span>
                            <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right" viewBox="0 0 284.49 498.98">
                              <path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path>
                            </svg>
                            <span class="icon__fallback-text">{{ 'general.drawers.expand_submenu' | t }}</span>
                          </span>
                        </button>
                        <ul class="slide-nav__dropdown" data-parent="tier-3-{{ grand_child_link_id }}" data-level="3">
                          <li class="slide-nav__item">
                            <button type="button" class="slide-nav__button js-toggle-submenu" data-title="{{ link.title | escape }}" data-target="tier-2-{{ child_list_handle }}">
                              <span class="slide-nav__link slide-nav__link--back">
                                <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-left" viewBox="0 0 284.49 498.98">
                                  <path d="M249.49 0a35 35 0 0 1 24.75 59.75L84.49 249.49l189.75 189.74a35.002 35.002 0 1 1-49.5 49.5L10.25 274.24a35 35 0 0 1 0-49.5L224.74 10.25A34.89 34.89 0 0 1 249.49 0z"></path>
                                </svg>
                                <span>{{ 'general.drawers.mobile_back_arrow_text' | t }}</span>
                              </span>
                            </button>
                          </li>
                          {% comment %}
                            If a collection, automatically add 'view all' link
                          {% endcomment %}
                          {%- if childlink.url contains '/collections/' -%}
                            <li class="slide-nav__item">
                              <a href="{{ childlink.url }}" class="slide-nav__link">
                                <span>{{ 'collections.general.all_of_collection' | t }}</span>
                                {%- if childlink.levels > 0 -%}
                                  <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right" viewBox="0 0 284.49 498.98">
                                    <path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path>
                                  </svg>
                                {%- endif -%}
                              </a>
                            </li>
                          {%- endif -%}
                          {%- for grandchildlink in childlink.links -%}
                            <li class="slide-nav__item mobile-nav__item">
                              <a href="{{ grandchildlink.url }}" class="slide-nav__link">
                                <span>{{ grandchildlink.title | escape }}</span>
                               </a>
                            </li>
                          {%- endfor -%}
                        </ul>
                      {%- else -%}
                        <a href="{{ childlink.url }}" class="slide-nav__link">
                          <span>{{ childlink.title | escape }}</span>
                          {%- unless childlink.levels == 0 -%}
                            <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right" viewBox="0 0 284.49 498.98">
                              <path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path>
                            </svg>
                          {%- endunless -%}
                        </a>
                      {%- endif -%}
                    </li>
                  {%- endfor -%}
                </ul>
              {%- else -%}
                <a href="{{ link.url }}" class="slide-nav__link mobile-nav__link--top-level">
                  <span>{{ link.title | escape }}</span>
                </a>
              {%- endif -%}
            </li>
          {%- endfor -%}
        </ul>
      </div>
      {% comment %}
         bg--custom
      {% endcomment %}
      <div class="drawer__scrollable">
        {%- liquid
           assign have_secondary_list = false
           
           if object.settings.toolbar_menu != blank or shop.customer_accounts_enabled
             assign have_secondary_list = true
           endif
        -%}

        <h4 class="drawer-section--heading">{{ 'general.drawers.secondary_menu.title' | t }}</h4>
        {%- if have_secondary_list -%}
          <div class="mobile-nav__item mobile-nav__item--secondary">
            <div class="grid">
              {%- if section.settings.toolbar_menu != blank -%}
                {%- for link in toolbar_menu.links -%}
                  {%- assign animation_row = animation_row | plus: 1 -%}
                  <div class="grid__item appear-animation appear-delay-{{ animation_row }} medium-up--hide">
                    <a href="{{ link.url }}" class="mobile-nav__link">{{ link.title }}</a>
                  </div>
                {%- endfor -%}
              {%- endif -%}
              {% comment %}
              {%- if shop.customer_accounts_enabled -%}
                {%- assign animation_row = animation_row | plus: 1 -%}
                <div class="grid__item one-half appear-animation appear-delay-{{ animation_row }}">
                  <a href="{{ routes.account_url }}" class="mobile-nav__link">
                    {%- if customer -%}
                      {{ 'layout.customer.account' | t }}
                    {%- else -%}
                      {% comment %}{{ 'layout.customer.log_in' | t }}{% endcomment %}
                      Account
                    {%- endif -%}
                  </a>
                </div>
              {%- endif -%}
              {% endcomment %}
              
            </div>
          </div>
        {%- endif -%}

        {% comment %} cepta-ls-start {% endcomment %}
        <h4 class="drawer-section--heading drawer-section--heading__account">{{ 'customer.account.title' | t }}</h4>
        <div class="mobile-nav__item mobile-nav__item--secondary">
          <div class="grid">
            {%- assign animation_row = animation_row | plus: 1 -%}
            <div class="grid__item appear-animation appear-delay-{{ animation_row }} medium-up--hide">
              <a href="{{ routes.cart_url }}" class="mobile-nav__link">
                <span class="mobile-nav__link--cart">{{ 'layout.cart.title' | t }}</span>
                <span class="cart__bubble--paran cart__bubble--paran__left">&#40;</span>
                <span data-cart-count="{{ cart.item_count }}" class="cart-link__bubble{% if cart.item_count > 0 %} cart-link__bubble--visible{% endif %}">
                  {{ cart.item_count }}
                </span>
                <span class="cart__bubble--paran cart__bubble--paran__right">&#41;</span>
              </a>
            </div>
            {%- if shop.customer_accounts_enabled -%}
              {%- assign animation_row = animation_row | plus: 1 -%}
              <div class="grid__item appear-animation appear-delay-{{ animation_row }} medium-up--hide">
                <a class="mobile-nav__link" href="{{ routes.account_url }}">
                  {%- if customer -%}
                    {{ 'layout.customer.account' | t }} - {{ customer.name }}
                  {%- else -%}
                    {{ 'layout.customer.account' | t }} - {{ 'layout.customer.log_in' | t }}
                  {%- endif -%}
                </a>
              </div>
            {%- endif -%}
            {%- unless customer -%}
              {%- assign animation_row = animation_row | plus: 1 -%}
              <div class="grid__item appear-animation appear-delay-{{ animation_row }} medium-up--hide">
                <a href="{{ routes.account_register_url }}" class="mobile-nav__link">{{ 'layout.customer.create_account' | t }}</a>
              </div>
            {%- endunless -%}
          </div>
        </div>
        {% comment %} cepta-ls-end {% endcomment %}

        <div class="visually-hidden">
          <localization-form>
            {%- liquid
               assign formId = 'localization_form-header'
               assign downcase_locale_iso = localization.language.iso_code | downcase
               assign locale_flag_class = 'currency-flag--' | append: downcase_locale_iso
               if downcase_locale_iso == 'en'
                 assign locale_flag_class = 'currency-flag--gb'
               elsif downcase_locale_iso == 'sv'
                 assign locale_flag_class = 'currency-flag--se'
               endif
            -%}
            {%- form 'localization', id: formId, class: 'multi-selectors' -%}
              {%- capture language_button -%}
                <button type="button" data-target="tier-2-lang" class="disclosure__button lang-switcher--btn" aria-expanded="false" aria-controls="LanguageList">
                  {% comment %}
                  <span class="currency-flag {{ locale_flag_class }}" data-flag="{{ localization.country.iso_code }}" aria-hidden="true"></span>
                  {% endcomment %}
                  <span class="country--name">
                    <span>{{ localization.language.endonym_name | capitalize }}</span>
                    <span class="localization--identifier">{{ 'general.language.dropdown_label' | t }}</span>
                  </span>
                  <span class="icon-wrapper">
                    <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right" viewBox="0 0 284.49 498.98">
                      <path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path>
                    </svg>
                  </span>
                </button>
              {%- endcapture -%}
              {%- capture currency_button -%}
                <button type="button" data-target="tier-2-cur" class="disclosure__button lang-switcher--btn" aria-expanded="false" aria-controls="CountryList">
                  {% comment %}
                  <span class="currency-flag currency-flag--{{ localization.country.iso_code | downcase }}" data-flag="{{ localization.country.currency.iso_code }}" aria-hidden="true"></span>
                  {% endcomment %}
                  <span class="country--name">
                    <span>
                      {{ localization.country.name }} ({{ localization.country.currency.iso_code }} {{ localization.country.currency.symbol }})
                    </span>
                    <span class="localization--identifier">{{ 'general.currency.dropdown_label' | t }}</span>
                  </span>
                  <span class="icon-wrapper">
                    <svg aria-hidden="true" focusable="false" role="presentation" class="icon icon-chevron-right" viewBox="0 0 284.49 498.98">
                      <path d="M35 498.98a35 35 0 0 1-24.75-59.75l189.74-189.74L10.25 59.75a35.002 35.002 0 0 1 49.5-49.5l214.49 214.49a35 35 0 0 1 0 49.5L59.75 488.73A34.89 34.89 0 0 1 35 498.98z"></path>
                    </svg>
                  </span>
                </button>
              {%- endcapture -%}
              {%- capture language_switcher -%}
                <ul class="slide-nav">
                  {%- for locale in form.available_locales -%}
                    {%- liquid
                       assign downcase_locale_iso = locale.iso_code | downcase
                       assign locale_flag_class = 'currency-flag--' | append: downcase_locale_iso
                       if downcase_locale_iso == 'en'
                         assign locale_flag_class = 'currency-flag--gb'
                       elsif downcase_locale_iso == 'sv'
                         assign locale_flag_class = 'currency-flag--se'
                       endif
                    -%}
                    <li class="slide-nav__item mobile-nav__item appear-animation appear-delay-1">
                      <span class="slide-nav__link  mobile-nav__link--top-level">
                        <button class="localization-toggle--btn" type="button" lang="{{ locale.iso_code }}"{% if locale.iso_code == form.current_locale.iso_code %} aria-current="true"{% endif %} data-type="language" data-value="{{ locale.iso_code }}">
                          {% comment %}
                          <span class="currency-flag {{ locale_flag_class }}" data-flag="{{ locale.iso_code }}" aria-hidden="true"></span>
                          {% endcomment %}
                          <div class="currency-checkbox-container">
                            <span class="currency-checkbox" tabindex="0"></span>
                          </div>
                          <span class="country--name">
                            <span>{{ locale.endonym_name | capitalize }}</span>
                          </span>
                          {% comment %}
                          <span class="is-selected--icon">
                            <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="50" height="50" viewBox="0 0 50 50">
                              <path d="M 41.9375 8.625 C 41.273438 8.648438 40.664063 9 40.3125 9.5625 L 21.5 38.34375 L 9.3125 27.8125 C 8.789063 27.269531 8.003906 27.066406 7.28125 27.292969 C 6.5625 27.515625 6.027344 28.125 5.902344 28.867188 C 5.777344 29.613281 6.078125 30.363281 6.6875 30.8125 L 20.625 42.875 C 21.0625 43.246094 21.640625 43.410156 22.207031 43.328125 C 22.777344 43.242188 23.28125 42.917969 23.59375 42.4375 L 43.6875 11.75 C 44.117188 11.121094 44.152344 10.308594 43.78125 9.644531 C 43.410156 8.984375 42.695313 8.589844 41.9375 8.625 Z"></path>
                            </svg>
                          </span>
                          {% endcomment %}
                        </button>
                      </span>
                    </li>
                  {%- endfor -%}
                </ul>
              {%- endcapture -%}
              {%- capture currency_switcher -%}
                <ul class="slide-nav">

                  {%- for country in form.available_countries-%}
                    <li class="slide-nav__item mobile-nav__item appear-animation appear-delay-1" data-country="{{ country.name }}" data-currency-iso="{{ country.currency.iso_code }}" data-currency-symbol="{{ country.currency.symbol }}">
                      <span class="slide-nav__link  mobile-nav__link--top-level">
                        <button class="localization-toggle--btn" type="button" lang="{{ locale.iso_code }}"{% if country.iso_code == localization.country.iso_code %} aria-current="true"{% endif %} data-type="currency" data-value="{{ country.iso_code }}">
                          {% comment %}
                          <span class="currency-flag currency-flag--{{ country.iso_code | downcase }} {% if country.iso_code == 'sv' %}currency-flag--se{% endif %}" data-flag="{{ country.currency.iso_code }}" aria-hidden="true"></span>
                          {% endcomment %}
                          <div class="currency-checkbox-container">
                            <span class="currency-checkbox" tabindex="0"></span>
                          </div>
                          <span class="country--name">
                            <span>{{ country.name }} ({{ country.currency.iso_code }} {{ country.currency.symbol }})</span>
                          </span>
                          {% comment %}
                          <span class="is-selected--icon">
                            <svg xmlns="http://www.w3.org/2000/svg" x="0px" y="0px" width="50" height="50" viewBox="0 0 50 50">
                              <path d="M 41.9375 8.625 C 41.273438 8.648438 40.664063 9 40.3125 9.5625 L 21.5 38.34375 L 9.3125 27.8125 C 8.789063 27.269531 8.003906 27.066406 7.28125 27.292969 C 6.5625 27.515625 6.027344 28.125 5.902344 28.867188 C 5.777344 29.613281 6.078125 30.363281 6.6875 30.8125 L 20.625 42.875 C 21.0625 43.246094 21.640625 43.410156 22.207031 43.328125 C 22.777344 43.242188 23.28125 42.917969 23.59375 42.4375 L 43.6875 11.75 C 44.117188 11.121094 44.152344 10.308594 43.78125 9.644531 C 43.410156 8.984375 42.695313 8.589844 41.9375 8.625 Z"></path>
                            </svg>
                          </span>
                          {% endcomment %}
                        </button>
                      </span>
                    </li>
                  {%- endfor -%}
                </ul>
              {%- endcapture -%}
              <input type="hidden" name="country_code" value="{{ localization.country.iso_code }}">
              <input type="hidden" name="language_code" value="{{ localization.language.iso_code }}">
            {%- endform -%}
          </localization-form>
        </div>

        {%- assign animation_row = animation_row | plus: 1 -%}
        <h4 class="drawer-section--heading drawer-section--heading__language">{{ 'general.language_currency.title' | t }}</h4>
        <div class="lang-currency--switcher">
          <div class="lang-currency--switcher__wrapper" data-level="1">
            {%- form 'localization', id: 'local-form-header', class: 'multi-selectors', data-disclosure-form: '' -%}
            <ul class="lang-cur--nav">
              <li class="slide-nav__item appear-animation appear-delay-2">
                {{ language_button }}
              </li>
              <li class="slide-nav__item appear-animation appear-delay-2">
                {{ currency_button }}
              </li>
            </ul>
            {%- endform -%}
          </div>
        </div>
        <ul class="mobile-nav__social appear-animation appear-delay-{{ animation_row }}">
          {%- if section.settings.toolbar_social -%}
            {%- if settings.social_instagram_link != blank -%}
              <li class="mobile-nav__social-item">
